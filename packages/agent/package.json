{
  "name": "@sphereon/ssi-web-wallet-agent",
  "private": true,
  "version": "0.1.0",
  "description": "Sphereon - Web Wallet Agent",
  "author": "Sphereon <dev@sphereon.com>",
  "keywords": [
    "VC API"
  ],
  "source": "src/index.ts",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "build:clean": "tsc --build --clean && tsc --build",
    "start:prod": "node --experimental-specifier-resolution=node dist/index.js",
    "start:dev": "node --experimental-specifier-resolution=node --loader ts-node/esm src/agent.ts",
    "start:debug": "pnpm -r --stream start:dev --trace-warnings",
    "demo:init": "cross-env RUN_MODE=cli node --experimental-specifier-resolution=node --loader ts-node/esm src/database/demo-init.ts",
    "db:migrate:down": "cross-env RUN_MODE=cli node --experimental-specifier-resolution=node --loader ts-node/esm src/database/db-command.ts revert-migration",
    "db:drop": "cross-env RUN_MODE=cli node --experimental-specifier-resolution=node --loader ts-node/esm src/database/db-command.ts drop-db"
  },
  "files": [
    "dist/**/*",
    "src/**/*",
    "README.md",
    "plugin.schema.json",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "restricted"
  },
  "prettier": {
    "endOfLine": "auto",
    "semi": false,
    "singleQuote": true,
    "printWidth": 150
  },
